//3
#include <iostream>
using namespace std;

int main() {
    float a[15];
    for (int i = 0; i < 15; i++) {
    cin >> a[i];
    }
    int mn= 0, mx = 0;
    for (int i = 0; i < 15; i++) {
        if (a[i] < a[mn]) mn = i;
        if (a[i] > a[mx]) mx = i;
    }
    swap(a[0], a[mn]);
    swap(a[14], a[mx]);
    for (int i = 0; i < 15; i++) {
    cout << a[i] << " "
    }
    return 0;
}




//4
#include <iostream>
using namespace std;

int main() {
    float a[15];
    for (int i = 0; i < 15; i++) {
        cin >> a[i];
    }
    int mn = 0, mx = 0;
    for (int i = 0; i < 15; i++) {
        if (a[i] < a[mn]) mn = i;
        if (a[i] > a[mx]) mx = i;
    }
    if (mx < mn) {
        cout << a[mn] * a[mx] << endl;
    }
    else {
        cout << "-" << endl;
    }
    return 0;
}



//5
#include <iostream>
using namespace std;
int main() {
    int a[10];
    for (int i = 0; i < 10; i++) {
        cin >> a[i];
    }
    int mn = 0, mx = 0;
    for (int i = 0; i < 10; i++) {
        if (a[i] < a[mn]) mn = i;
        if (a[i] > a[mx]) mx = i;
    }
    if (mn == 4 && mx == 9) {
        cout << (a[mn] + a[mx]) / 2.0 << endl;
    }
    else {
    cout << "-";
    }
    return 0;
}




