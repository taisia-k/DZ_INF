//8

#include <iostream>
using namespace std;
int main() {
    int a[10];
    for (int i = 0; i < 10; i++) {
        cin >> a[i];
    }
    int mn = 0, mx = 0;
    for (int i = 0; i < 10; i++) {
        if (a[i] < a[mn]) mn = i;
        if (a[i] > a[mx]) mx = i;
    }
    if (mx == 1 && mn == 4) {
        for (int i = mn; i < 10; i++) {
            a[i] = a[mx];
        }
    }
    for (int i = 0; i < 10; i++) {
        cout << a[i] << " ";
    }
    return 0;
}
